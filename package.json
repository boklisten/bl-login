{
	"name": "@wizardcoder/bl-login",
	"version": "0.3.1",
	"license": "MIT",
	"$schema": "./node_modules/ng-packagr/package.schema.json",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --aot --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en",
		"start-nb": "ng serve --aot --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb",
		"build": "ng build --aot --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"extract-i18n": "ng xi18n --output-path src/locale --locale en && xliffmerge --profile xliffmerge.json en nb",
		"build:lib": "ng-packagr -p package.json",
		"lib:pub": "npm run build:lib && npm publish ./dist/@wizardcoder/bl-login",
		"blink": "npm run build:lib && npm link dist/bl-login"
	},
	"ngPackage": {
		"lib": {
			"entryFile": "public_api.ts"
		},
		"dest": "./dist/@wizardcoder/bl-login"
	},
	"devDependencies": {
		"@angular/animations": "^5.1.0",
		"@angular/cli": "1.7.0-beta.2",
		"@angular/common": "^5.1.0",
		"@angular/compiler": "^5.1.0",
		"@angular/compiler-cli": "^5.1.0",
		"@angular/core": "^5.1.0",
		"@angular/forms": "^5.1.0",
		"@angular/http": "^5.1.0",
		"@angular/language-service": "^5.1.0",
		"@angular/platform-browser": "^5.1.0",
		"@angular/platform-browser-dynamic": "^5.1.0",
		"@angular/router": "^5.1.0",
		"@auth0/angular-jwt": "^1.0.0-beta.10",
		"@fortawesome/angular-fontawesome": "0.1.0-8",
		"@fortawesome/fontawesome": "^1.1.3",
		"@fortawesome/fontawesome-svg-core": "^1.2.0-10",
		"@fortawesome/free-brands-svg-icons": "^5.1.0-4",
		"@fortawesome/free-regular-svg-icons": "^5.1.0-4",
		"@fortawesome/free-solid-svg-icons": "^5.1.0-4",
		"@ng-bootstrap/ng-bootstrap": "^1.0.0",
		"@types/jasmine": "~2.5.53",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "~6.0.60",
		"angular-2-local-storage": "^1.0.1",
		"codelyzer": "~3.1.1",
		"core-js": "^2.4.1",
		"email-validator": "^1.1.1",
		"jasmine-core": "~2.6.2",
		"jasmine-spec-reporter": "~4.1.0",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.1.1",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"moment-es6": "^1.0.0",
		"ng-packagr": "^2.4.2",
		"ngx-cookie": "^2.0.1",
		"ngx-i18nsupport": "^0.16.2",
		"protractor": "~5.1.2",
		"rxjs": "^5.4.2",
		"ts-node": "~3.2.0",
		"tslint": "~5.3.2",
		"typescript": "~2.4.2",
		"zone.js": "^0.8.14"
	},
	"peerDependencies": {
		"@angular/common": "^5.1.0",
		"@angular/core": "^5.1.0",
		"@angular/forms": "^5.1.0",
		"@angular/router": "^5.1.0",
		"@ng-bootstrap/ng-bootstrap": "^1.0.0",
		"@wizardcoder/bl-connect": "^0.5.3",
		"@wizardcoder/bl-model": "^0.14.7",
		"moment": "^2.20.1",
		"email-validator": "^1.1.1",
		"@fortawesome/angular-fontawesome": "0.1.0-8",
		"@fortawesome/fontawesome-svg-core": "^1.2.0-10",
		"@fortawesome/free-brands-svg-icons": "^5.1.0-4",
		"@fortawesome/free-regular-svg-icons": "^5.1.0-4",
		"@fortawesome/free-solid-svg-icons": "^5.1.0-4"
	},
	"dependencies": {
		"@wizardcoder/bl-connect": "^0.5.13"
	}
}
