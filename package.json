{
	"name": "@wizardcoder/bl-login",
	"version": "0.6.5",
	"license": "MIT",
	"$schema": "./node_modules/ng-packagr/package.schema.json",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start-nb": "ng serve --aot --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb",
		"build": "ng build --aot --i18nFile=src/locale/messages.nb.xlf --i18nFormat=xlf --locale=nb",
		"test": "ng test --sourcemaps=false",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"extract-i18n": "ng xi18n --output-path src/locale --locale en && xliffmerge --profile xliffmerge.json en nb",
		"build:lib": "ng-packagr -p package.json",
		"lib:pub": "npm run build:lib && npm publish ./dist/@wizardcoder/bl-login",
		"blink": "npm run build:lib && npm link dist/bl-login"
	},
	"ngPackage": {
		"lib": {
			"entryFile": "public_api.ts"
		},
		"dest": "./dist/@wizardcoder/bl-login",
		"whitelistedNonPeerDependencies": [
			"@auth0/angular-jwt",
			"@wizardcoder/bl-connect",
			"date-fns",
			"ng2-datepicker"
		]
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.8",
		"@angular/animations": "6.0.6",
		"@angular/cli": "6.0.8",
		"@angular/common": "6.0.6",
		"@angular/compiler": "6.0.6",
		"@angular/compiler-cli": "6.0.6",
		"@angular/core": "6.0.6",
		"@angular/forms": "6.0.6",
		"@angular/http": "6.0.6",
		"@angular/language-service": "6.0.6",
		"@angular/platform-browser": "6.0.6",
		"@angular/platform-browser-dynamic": "6.0.6",
		"@angular/router": "6.0.6",
		"@fortawesome/angular-fontawesome": "0.1.0-10",
		"@fortawesome/fontawesome": "^1.1.8",
		"@fortawesome/fontawesome-svg-core": "^1.2.2",
		"@fortawesome/free-brands-svg-icons": "^5.2.0",
		"@fortawesome/free-regular-svg-icons": "^5.2.0",
		"@fortawesome/free-solid-svg-icons": "^5.2.0",
		"@ng-bootstrap/ng-bootstrap": "^2.2.1",
		"@types/jasmine": "~2.5.53",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "^6.0.115",
		"@wizardcoder/bl-model": "^0.14.7",
		"codelyzer": "~3.1.1",
		"core-js": "^2.5.7",
		"email-validator": "^1.2.3",
		"jasmine-core": "~2.6.2",
		"jasmine-spec-reporter": "~4.1.0",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.1.1",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.4.3",
		"karma-jasmine": "^1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"moment": "^2.20.1",
		"moment-es6": "^1.0.0",
		"ng-packagr": "^3.0.6",
		"ngx-cookie": "^2.0.1",
		"ngx-i18nsupport": "^0.16.3",
		"protractor": "^5.4.0",
		"rxjs": "^6.2.2",
		"ts-node": "~3.2.0",
		"tslint": "~5.3.2",
		"typescript": "2.7.2",
		"zone.js": "^0.8.26"
	},
	"peerDependencies": {
		"@angular/common": "6.0.6",
		"@angular/core": "6.0.6",
		"@angular/forms": "6.0.6",
		"@angular/router": "6.0.6",
		"moment": "^2.20.1",
		"email-validator": "^1.1.1",
		"@ng-bootstrap/ng-bootstrap": "^2.1.2",
		"@fortawesome/angular-fontawesome": "^0.1.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.2",
		"@fortawesome/free-brands-svg-icons": "^5.2.0",
		"@fortawesome/free-regular-svg-icons": "^5.2.0",
		"@fortawesome/free-solid-svg-icons": "^5.2.0",
		"@wizardcoder/bl-connect": "^0.6.1",
		"@wizardcoder/bl-model": "^0.14.7"
	},
	"dependencies": {
		"@auth0/angular-jwt": "^2.0.0",
		"@wizardcoder/bl-connect": "^0.7.2",
		"date-fns": "^1.29.0",
		"ng2-datepicker": "^3.1.0"
	}
}
