{
	"name": "@boklisten/bl-login",
	"version": "1.2.3",
	"license": "MIT",
	"$schema": "./node_modules/ng-packagr/package.schema.json",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"watch": "ng serve",
		"start-nb": "ng serve",
		"build": "ng build",
		"test": "ng test --sourcemaps=false",
		"lint": "eslint . --ignore-path .gitignore --fix",
		"prettier": "prettier --write . --ignore-path .gitignore",
		"extract-i18n": "ng xi18n && xliffmerge --profile xliffmerge.json en nb",
		"build:lib": "ng-packagr -p package.json",
		"pub": "npm run build:lib && npm publish ./dist/@boklisten/bl-login",
		"blink": "npm run build:lib && npm link dist/bl-login"
	},
	"ngPackage": {
		"lib": {
			"entryFile": "public_api.ts"
		},
		"dest": "./dist/@boklisten/bl-login",
		"whitelistedNonPeerDependencies": [
			"@auth0/angular-jwt",
			"@boklisten/bl-connect",
			"@boklisten/bl-model",
			"date-fns"
		]
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1101.4",
		"@angular/animations": "^11.1.2",
		"@angular/cli": "^15.0.5",
		"@angular/common": "^11.1.2",
		"@angular/compiler": "^11.1.2",
		"@angular/compiler-cli": "^11.1.2",
		"@angular/core": "^11.1.2",
		"@angular/forms": "^11.1.2",
		"@angular/language-service": "^11.1.2",
		"@angular/localize": "^11.1.2",
		"@angular/platform-browser": "^11.1.2",
		"@angular/platform-browser-dynamic": "^11.1.2",
		"@angular/router": "^11.1.2",
		"@boklisten/bl-model": "^0.25.0",
		"@fortawesome/angular-fontawesome": "^0.8.2",
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-regular-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@ng-bootstrap/ng-bootstrap": "^9.0.2",
		"@types/jasmine": "~3.6.3",
		"@types/jasminewd2": "~2.0.8",
		"@types/node": "^14.14.25",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"codelyzer": "^6.0.1",
		"core-js": "^3.8.3",
		"email-validator": "2.0.4",
		"eslint": "^7.19.0",
		"file-saver": "^2.0.5",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"karma": "~6.1.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-cli": "~2.0.0",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"moment": "^2.29.1",
		"moment-es6": "^1.0.0",
		"ng-packagr": "^11.2.0",
		"ngx-cookie": "^5.0.1",
		"ngx-i18nsupport": "^0.17.1",
		"prettier": "^2.2.1",
		"protractor": "~7.0.0",
		"rxjs": "^6.6.3",
		"ts-node": "~9.1.1",
		"tslint": "~6.1.0",
		"typescript": "4.1.4",
		"zone.js": "~0.11.3"
	},
	"dependencies": {
		"@auth0/angular-jwt": "^5.0.2",
		"@boklisten/bl-connect": "^0.19.0",
		"date-fns": "^2.17.0",
		"tslib": "^2.1.0"
	},
	"description": "`bl-login` is a login and registration module for Boklisten.no. This README is for development and technical documentation. For a deep-dive into what   `bl-login` is and can do [please read our   documentation](https://github.com/boklisten/bl-doc/blob/master/bl-login#summary)",
	"main": "karma.conf.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/boklisten/bl-login.git"
	},
	"author": "",
	"bugs": {
		"url": "https://github.com/boklisten/bl-login/issues"
	},
	"homepage": "https://github.com/boklisten/bl-login#readme"
}
